APP_ENV=local
APP_NAME=vibe-trading
LOG_LEVEL=INFO

DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/vibetrading
REDIS_URL=redis://localhost:6379/0
TASK_QUEUE_ENABLED=false
TASK_QUEUE_NAME=vibe-runs
TASK_QUEUE_JOB_TIMEOUT_SECONDS=7200
TASK_QUEUE_RECOVERY_LOOKBACK_HOURS=24

SUPABASE_PROJECT_URL=
SUPABASE_SECRET_KEY=
AUTH_MODE=local_jwt
AUTH_TOKEN_CACHE_MAX_ENTRIES=4096
SUPABASE_JWT_ISSUER=
SUPABASE_JWT_AUDIENCES=authenticated
SUPABASE_JWKS_CACHE_TTL_SECONDS=600
SUPABASE_STORAGE_ENABLED=false
SUPABASE_STORAGE_BUCKET=run-artifacts
SUPABASE_STORAGE_SIGNED_URL_TTL_SECONDS=3600

LLM_BASE_URL=https://api.openai.com/v1
LLM_API_KEY=
LLM_MODEL=gpt-5-mini
LLM_REQUEST_TIMEOUT_SECONDS=120
LLM_MAX_RETRIES=2
LLM_RETRY_BACKOFF_SECONDS=1.0
LLM_SEMANTIC_REPAIR_ATTEMPTS=1

MARKET_DATA_PROVIDER=alpaca
ALPACA_DATA_BASE_URL=https://data.alpaca.markets
ALPACA_DATA_FEED=iex
ALPACA_PAPER_API_ENDPOINT=https://paper-api.alpaca.markets
ALPACA_PAPER_API_KEY=
ALPACA_PAPER_API_SECRET=

ALLOWED_ORIGINS=http://localhost:5173
